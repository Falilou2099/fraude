# Base de données PostgreSQL RDS
DATABASE_URL="postgresql://username:password@your-rds-endpoint.region.rds.amazonaws.com:5432/component_dashboard"

# Environnement
NODE_ENV="production"

# Port de l'application
PORT=3000

# Clé secrète pour les sessions (générer une clé forte en production)
SESSION_SECRET="your-super-secret-session-key-change-this-in-production"

# Configuration AWS (optionnel si utilisation d'IAM roles)
AWS_REGION="eu-west-1"
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
